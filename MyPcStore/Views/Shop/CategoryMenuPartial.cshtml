@model IEnumerable<MyPcStore.Models.ViewModels.Shop.CategoryVM>

<h3>Categories</h3>

<ul>
    @foreach (var item in Model)
    {    
        <li style="list-style-type:none;"><a href="/shop/category/@item.Slug.ToLower()"><i class="far fa-caret-square-right"></i>  @item.Name</a></li>  
    }
</ul>

